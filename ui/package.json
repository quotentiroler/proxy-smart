{
  "name": "proxy-smart-ui",
  "displayName": "Proxy Smart Admin UI",
  "description": "A web-based administration interface for managing healthcare applications and resources via Proxy Smart.",
  "private": true,
  "version": "0.0.1-alpha.202512271341.80e0c4c9",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "typecheck": "tsc -b",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate": "cd ../backend && bun run scripts/fix-openapi-spec.ts && cd ../ui && bunx --bun @openapitools/openapi-generator-cli generate -i ../backend/dist/openapi.json -g typescript-fetch -o ./src/lib/api-client --skip-validate-spec",
    "validate-api": "bunx @openapitools/openapi-generator-cli validate -i http://localhost:8445/swagger/json",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
