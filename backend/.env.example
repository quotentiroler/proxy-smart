# Base URL for your SMART proxy
BASE_URL=http://localhost:8445

# Keycloak configuration
KEYCLOAK_BASE_URL=http://localhost:8080
KEYCLOAK_REALM=your-realm
KEYCLOAK_CLIENT_ID=your-client-id
KEYCLOAK_CLIENT_SECRET=your-client-secret
KEYCLOAK_JWKS_URI=http://localhost:8080/realms/your-realm/protocol/openid-connect/certs

# FHIR server configuration
FHIR_SERVER_BASE=https://hapi.fhir.org/baseR4
FHIR_SUPPORTED_VERSIONS=STU3, R4, R5

# Optional: Custom port (defaults to 8445)
PORT=8445
