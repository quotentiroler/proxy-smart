{
  "name": "proxy-smart-backend",
  "displayName": "Proxy Smart Backend",
  "version": "0.0.1-alpha.202510170045.f4d64f5",
  "type": "module",
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "tsc --noEmit && bun build src/index.ts --outdir dist --target node",
    "export-openapi": "mkdir -p dist && bun run src/export-openapi.ts && bun run scripts/sanitize-openapi.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/eden": "^1.4.1",
    "@elysiajs/openapi": "^1.4.11",
    "@elysiajs/static": "^1.4.0",
    "@keycloak/keycloak-admin-client": "^26.2.5",
    "cross-fetch": "^4.1.0",
    "elysia": "^1.4.9",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "node-fetch": "^3.3.2",
    "node-forge": "^1.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@socketsecurity/bun-security-scanner": "^1.1.0",
    "@types/bun": "^1.2.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node-fetch": "^2.6.13",
    "@types/node-forge": "^1.3.13",
    "bun-types": "latest",
    "eslint": "^9.30.0",
    "globals": "^16.2.0",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.35.0"
  },
  "module": "src/index.js"
}
