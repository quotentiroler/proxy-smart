{
  "name": "proxy-smart-backend",
  "displayName": "Proxy Smart Backend",
  "version": "0.0.2-alpha.202512282249.129c7cda",
  "type": "module",
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
    "dev": "bun run --watch --watch-path src src/index.ts",
    "start": "bun run src/index.ts",
    "mcp": "bun run src/mcp-server.ts",
    "build": "tsc --noEmit && bun build src/index.ts --outdir dist --target node",
    "export-openapi": "mkdir -p dist && bun run src/export-openapi.ts && bun run scripts/sanitize-openapi.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.1",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/eden": "^1.4.6",
    "@elysiajs/openapi": "^1.4.12",
    "@elysiajs/static": "^1.4.7",
    "@keycloak/keycloak-admin-client": "^26.4.7",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@sinclair/typebox": "^0.34.45",
    "ai": "^6.0.3",
    "cross-fetch": "^4.1.0",
    "elysia": "^1.4.19",
    "jsonwebtoken": "^9.0.3",
    "jwks-rsa": "^3.2.0",
    "node-fetch": "^3.3.2",
    "node-forge": "^1.3.3",
    "pg": "^8.16.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@socketsecurity/bun-security-scanner": "^1.1.2",
    "@types/bun": "^1.3.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node-fetch": "^2.6.13",
    "@types/node-forge": "^1.3.14",
    "@types/pg": "^8.16.0",
    "bun-types": "^1.3.5",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.50.1"
  },
  "module": "src/index.js"
}
