{
  "name": "proxy-smart-backend",
  "displayName": "Proxy Smart Backend",
  "version": "0.0.1-alpha.202511051116.e2d5e4cc",
  "type": "module",
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
    "dev": "bun run --watch --watch-path src src/index.ts",
    "start": "bun run src/index.ts",
    "mcp": "bun run src/mcp-server.ts",
    "build": "tsc --noEmit && bun build src/index.ts --outdir dist --target node",
    "export-openapi": "mkdir -p dist && bun run src/export-openapi.ts && bun run scripts/sanitize-openapi.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.56",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/eden": "^1.4.4",
    "@elysiajs/openapi": "^1.4.11",
    "@elysiajs/static": "^1.4.6",
    "@keycloak/keycloak-admin-client": "^26.4.4",
    "@modelcontextprotocol/sdk": "^1.21.1",
    "@sinclair/typebox": "^0.34.41",
    "ai": "^5.0.81",
    "cross-fetch": "^4.1.0",
    "elysia": "^1.4.13",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "node-fetch": "^3.3.2",
    "node-forge": "^1.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@socketsecurity/bun-security-scanner": "^1.1.1",
    "@types/bun": "^1.3.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node-fetch": "^2.6.13",
    "@types/node-forge": "^1.3.14",
    "bun-types": "latest",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "module": "src/index.js"
}
